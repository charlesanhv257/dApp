# Blockchain Environment Variables

# Private Keys (NEVER commit real private keys to git)
PRIVATE_KEY=your_private_key_here

# RPC URLs
ALCHEMY_API_KEY=your_alchemy_api_key_here
INFURA_API_KEY=your_infura_api_key_here

# Network URLs
SEPOLIA_URL=https://eth-sepolia.g.alchemy.com/v2/${ALCHEMY_API_KEY}
GOERLI_URL=https://eth-goerli.g.alchemy.com/v2/${ALCHEMY_API_KEY}
MAINNET_URL=https://eth-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas price settings
GAS_PRICE=20000000000

# Contract addresses (will be populated after deployment)
DAPP_TOKEN_ADDRESS=
DAPP_NFT_ADDRESS=
VOTING_CONTRACT_ADDRESS=
